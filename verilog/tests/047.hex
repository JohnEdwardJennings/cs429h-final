// This test loops 10 times, taking one path on even loops and another path on odd loops.

@0
80a1    // movl r1, #10     // number of loops
8012    // movl r2, #1      // Always 1
87a3    // movl r3, #122    // 'z'
8004    // movl r4, #0      // Parity (alternates between 0 and 1)
80c8    // movl r8, #12     // Address of start of loops
8109    // movl r9, #16     // Address of print (sub r0, r3, r1) which is only run when r4 = 1
e409    // jz r9, r4        // Location of start of loop; jump to two lines below this
0310    // sub r0, r3, r1   // Skipped if r4 = 0
0121    // sub r1, r1, r2   // Location of jz target; Decrement loop variable
0244    // sub r4, r2, r4   // Negate parity variable
e118    // jnz r8, r1       // Jump to start of loop
ffff    // end
